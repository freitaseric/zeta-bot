    "@types/bun": "^1.1.5",
    "@types/node": "^18.19.38",
    "prisma": "^5.15.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "might-log": "^0.1.6"
